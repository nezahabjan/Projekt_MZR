dvodelnost <- function(graf, start){
matrika <- get.adjacency(graf$network)
barve <- rep(0,dim(matrika)[1])
names(barve) <- rownames(matrika)
k=0
barve[start] <- k
sosedje <- neighbors(graf$network, start)
if (length(sosedje)>0){
for (sosed in sosedje){
k=k+1
barve[sosed] <- k
}
}
return (barve)
}
